{
  "name": "nomis-terminal",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "load:test": "bombardier -c 125 -n 10000 http://localhost:4202",
    "dev:ssr": "ng run nomis-terminal:serve-ssr --port=4202",
    "serve:ssr": "bun dist/nomis-terminal/server/main.js",
    "serve:ssr:node": "node dist/nomis-terminal/server/main.js",
    "build:ssr": "ng build && ng run nomis-terminal:server",
    "prerender": "ng run nomis-terminal:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0-next.3",
    "@angular/common": "^17.0.0-next.3",
    "@angular/compiler": "^17.0.0-next.3",
    "@angular/core": "^17.0.0-next.3",
    "@angular/forms": "^17.0.0-next.3",
    "@angular/platform-browser": "^17.0.0-next.3",
    "@angular/platform-browser-dynamic": "^17.0.0-next.3",
    "@angular/platform-server": "^17.0.0-next.3",
    "@angular/router": "^17.0.0-next.3",
    "@angular/ssr": "^17.0.0-next.1",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0-next.3",
    "@angular/cli": "~17.0.0-next.3",
    "@angular/compiler-cli": "^17.0.0-next.3",
    "@netlify/plugin-angular-universal": "^1.0.1",
    "@types/node": "^20.5.9",
    "typescript": "~5.1.6"
  }
}
